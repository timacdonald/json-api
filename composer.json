    "autoload": {
        "psr-4": {
            "TiMacDonald\\JsonApi\\": "src"
        }
    },
    "require": {
        "spatie/once": "^3.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "infection/infection": "^0.24.0",
        "nunomaduro/larastan": "^0.7.12",
        "orchestra/testbench": "^6.19",
        "phpstan/phpstan": "^0.12.94",
        "phpstan/phpstan-strict-rules": "^0.12.10",
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
